    "typed-error": "3.0.0",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.21",
    "@types/express": "4.16.0",
    "@types/lodash": "4.14.110",
    "@types/mocha": "5.2.4",
    "@types/node": "^10.17.46",
    "@types/uuid": "3.4.3",
    "mocha": "5.2.0",
    "standard": "11.0.1",
